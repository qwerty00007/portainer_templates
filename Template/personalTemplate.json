					"name": "NEXTCLOUD_DATA_PATH"
				},
				{
					"default":"/portainer/AppConfig/Nextcloud/Config",
					"label": "NEXTCLOUD_CONFIG_PATH",
					"name": "NEXTCLOUD_CONFIG_PATH"
				},
				{
					"default":"/portainer/AppConfig/Mariadb/Config",
					"label": "MARIADB_CONFIG_PATH",
					"name": "MARIADB_CONFIG_PATH"
				},
				{
					"default":"nextcloud_db",
					"label": "MYSQL_DATABASE",
					"name": "MYSQL_DATABASE"
				},
				{
					"default":"nextcloud",
					"label": "MYSQL_USER",
					"name": "MYSQL_USER"
				}

